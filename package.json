{
  "name": "json-brook",
  "version": "0.0.3",
  "description": "parse json data streamly",
  "keywords": ["json", "stream"],
  "homepage": "https://github.com/asurance/json-brook",
  "bugs": {
    "url": "https://github.com/asurance/json-brook/issues"
  },
  "license": "MIT",
  "author": "asurance",
  "sideEffects": [],
  "main": "./dist/lib/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "files": ["dist", "CHANGELOG.md"],
  "scripts": {
    "build": "modern build",
    "build:doc": "modern build --platform",
    "build:watch": "modern build -w",
    "bump": "modern bump",
    "change": "modern change",
    "change-status": "modern change-status",
    "dev": "modern dev",
    "gen-release-note": "modern gen-release-note",
    "lint": "biome check",
    "new": "modern new",
    "pre": "modern pre",
    "prepare": "modern build && simple-git-hooks",
    "release": "modern release",
    "reset": "npx rimraf node_modules ./**/node_modules",
    "sort": "sort-package-json",
    "test": "jest",
    "upgrade": "modern upgrade"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@modern-js/module-tools": "2.60.3",
    "@types/jest": "^29.5.14",
    "@types/node": "~16.11.7",
    "jest": "^29.7.0",
    "lint-staged": "~13.1.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "sort-package-json": "^2.10.1",
    "ts-jest": "^29.2.5",
    "typescript": "~5.0.4"
  },
  "packageManager": "pnpm@9.1.2",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
