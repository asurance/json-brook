{
	"name": "json-brook",
	"version": "0.1.0-alpha.2",
	"description": "parse json data streamly",
	"keywords": [
		"json",
		"stream"
	],
	"homepage": "https://github.com/asurance/json-brook",
	"bugs": {
		"url": "https://github.com/asurance/json-brook/issues"
	},
	"license": "MIT",
	"author": "asurance",
	"sideEffects": [],
	"type": "module",
	"files": [
		"dist",
		"CHANGELOG.md"
	],
	"main": "./dist/index.umd.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.umd.cjs"
		}
	},
	"scripts": {
		"dev": "vite --config vite.doc.config.ts",
		"build": "tsc && vite build",
		"build:doc": "tsc && vite build --config vite.doc.config.ts",
		"preview": "vite preview",
		"lint": "biome check --fix"
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
			"pnpm lint"
		]
	},
	"devDependencies": {
		"@biomejs/biome": "2.4.4",
		"@tailwindcss/vite": "^4.1.13",
		"@types/node": "^25.3.0",
		"lint-staged": "~16.2.7",
		"shiki": "^3.22.0",
		"simple-git-hooks": "^2.13.1",
		"solid-devtools": "^0.34.3",
		"solid-js": "^1.9.11",
		"tailwindcss": "^4.1.13",
		"typescript": "~5.9.3",
		"vite": "^7.3.1",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-solid": "^2.11.8"
	},
	"packageManager": "pnpm@9.1.2"
}
